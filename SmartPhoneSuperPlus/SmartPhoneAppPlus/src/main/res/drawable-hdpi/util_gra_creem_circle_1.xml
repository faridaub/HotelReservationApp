<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#ffe2ba"
                android:endColor="#ffe2ba"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#FFF" />

        </shape>
    </item>
</selector>

