<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#9eb6c5"
                android:endColor="#9eb6c5"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#fff" />
            <corners
                android:radius="2dp" />
            <padding
                android:left="10dp"
                android:top="5dp"
                android:right="10dp"
                android:bottom="5dp" />
        </shape>
    </item>
</selector>
