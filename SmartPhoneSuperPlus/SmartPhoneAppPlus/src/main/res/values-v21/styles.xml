<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="android:Theme.Material.Light">
    </style>

    <style name="FaridCalendarView" parent="@android:style/Widget.CalendarView">
        <item name="android:focusedMonthDateColor">@color/black</item>
        <item name="android:weekDayTextAppearance">@android:style/TextAppearance.Medium</item>
        <item name="android:dateTextAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:selectedWeekBackgroundColor">@android:color/transparent</item>
        <item name="android:weekNumberColor">@color/red</item>
    </style>

    <style name="TextContactContactName">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:shadowColor">#ffffff</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>


</resources>
